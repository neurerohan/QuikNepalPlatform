{
  "version": 2,
  "framework": null,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/public"
      }
    },
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "headers": {
        "Cache-Control": "no-store, no-cache, must-revalidate, proxy-revalidate",
        "Pragma": "no-cache",
        "Expires": "0"
      },
      "dest": "dist/index.js",
      "continue": false
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/^((?!api|assets|_next|favicon.ico).)*$/",
      "dest": "/index.html",
      "status": 200
    }
  ]
} 